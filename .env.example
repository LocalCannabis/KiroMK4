# Kiro Environment Variables
# Copy this to .env and fill in your values

# Log level: DEBUG, INFO, WARNING, ERROR
KIRO_LOG_LEVEL=INFO

# Database path (default: ~/.kiro/data/kiro.db)
# KIRO_DATABASE__PATH=/path/to/kiro.db

# LLM API Keys (Phase 2+)
# KIRO_LLM__ANTHROPIC_API_KEY=sk-ant-...
# KIRO_LLM__OPENAI_API_KEY=sk-...

# Audio settings (Phase 1+)
# KIRO_AUDIO__INPUT_DEVICE=default
# KIRO_AUDIO__OUTPUT_DEVICE=default
